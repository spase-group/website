         </xsd:enumeration>
         <xsd:enumeration value="Europa">
            <xsd:annotation>
               <xsd:documentation xml:lang="en">
                  
               </xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Ganymede">
