            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Rhea">
            <xsd:annotation>
               <xsd:documentation xml:lang="en">
                  
               </xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Titan">
            <xsd:annotation>
               <xsd:documentation xml:lang="en">
                  
               </xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Title">
